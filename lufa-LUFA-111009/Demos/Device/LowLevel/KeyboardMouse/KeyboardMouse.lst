 120:KeyboardMouse.c **** /** Event handler for the USB_ControlRequest event. This is used to catch and process control reque
 121:KeyboardMouse.c ****  *  the device from the USB host before passing along unhandled control requests to the library for
 122:KeyboardMouse.c ****  *  internally.
 123:KeyboardMouse.c ****  */
 124:KeyboardMouse.c **** void EVENT_USB_Device_ControlRequest(void)
 125:KeyboardMouse.c **** {
 126:KeyboardMouse.c **** 	uint8_t* ReportData;
 127:KeyboardMouse.c **** 	uint8_t  ReportSize;
 128:KeyboardMouse.c **** 
 129:KeyboardMouse.c **** 	/* Handle HID Class specific requests */
 130:KeyboardMouse.c **** 	switch (USB_ControlRequest.bRequest)
 131:KeyboardMouse.c **** 	{
 132:KeyboardMouse.c **** 		case HID_REQ_GetReport:
 133:KeyboardMouse.c **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_DEVICETOHOST | REQTYPE_CLASS | REQREC_INTERFACE)
 134:KeyboardMouse.c **** 			{
 135:KeyboardMouse.c **** 				Endpoint_ClearSETUP();
 136:KeyboardMouse.c **** 
 137:KeyboardMouse.c **** 				/* Determine if it is the mouse or the keyboard data that is being requested */
 138:KeyboardMouse.c **** 				if (!(USB_ControlRequest.wIndex))
 139:KeyboardMouse.c **** 				{
 140:KeyboardMouse.c **** 					ReportData = (uint8_t*)&KeyboardReportData;
 141:KeyboardMouse.c **** 					ReportSize = sizeof(KeyboardReportData);
 142:KeyboardMouse.c **** 				}
 143:KeyboardMouse.c **** 				else
 144:KeyboardMouse.c **** 				{
 145:KeyboardMouse.c **** 					ReportData = (uint8_t*)&MouseReportData;
 146:KeyboardMouse.c **** 					ReportSize = sizeof(MouseReportData);
 147:KeyboardMouse.c **** 				}
 148:KeyboardMouse.c **** 
 149:KeyboardMouse.c **** 				/* Write the report data to the control endpoint */
 150:KeyboardMouse.c **** 				Endpoint_Write_Control_Stream_LE(ReportData, ReportSize);
 151:KeyboardMouse.c **** 				Endpoint_ClearOUT();
 152:KeyboardMouse.c **** 
 153:KeyboardMouse.c **** 				/* Clear the report data afterwards */
 154:KeyboardMouse.c **** 				memset(ReportData, 0, ReportSize);
 155:KeyboardMouse.c **** 			}
 156:KeyboardMouse.c **** 
 157:KeyboardMouse.c **** 			break;
 158:KeyboardMouse.c **** 		case HID_REQ_SetReport:
 159:KeyboardMouse.c **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_HOSTTODEVICE | REQTYPE_CLASS | REQREC_INTERFACE)
 160:KeyboardMouse.c **** 			{
 161:KeyboardMouse.c **** 				Endpoint_ClearSETUP();
 162:KeyboardMouse.c **** 
 163:KeyboardMouse.c **** 				/* Wait until the LED report has been sent by the host */
 164:KeyboardMouse.c **** 				while (!(Endpoint_IsOUTReceived()))
 165:KeyboardMouse.c **** 				{
 166:KeyboardMouse.c **** 					if (USB_DeviceState == DEVICE_STATE_Unattached)
 167:KeyboardMouse.c **** 					  return;
 168:KeyboardMouse.c **** 				}
 169:KeyboardMouse.c **** 
 170:KeyboardMouse.c **** 				/* Read in the LED report from the host */
 171:KeyboardMouse.c **** 				uint8_t LEDStatus = Endpoint_Read_8();
 172:KeyboardMouse.c **** 
 173:KeyboardMouse.c **** 				Endpoint_ClearOUT();
 174:KeyboardMouse.c **** 				Endpoint_ClearStatusStage();
 175:KeyboardMouse.c **** 
 176:KeyboardMouse.c **** 				/* Process the incoming LED report */
 177:KeyboardMouse.c **** 				Keyboard_ProcessLEDReport(LEDStatus);
 178:KeyboardMouse.c **** 			}
 179:KeyboardMouse.c **** 
 180:KeyboardMouse.c **** 			break;
 181:KeyboardMouse.c **** 	}
 182:KeyboardMouse.c **** }
