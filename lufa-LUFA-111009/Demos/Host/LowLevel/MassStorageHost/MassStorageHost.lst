 164:MassStorageHost.c **** /** Task to set the configuration of the attached device after it has been enumerated, and to read 
 165:MassStorageHost.c ****  *  the device and print them to the serial port.
 166:MassStorageHost.c ****  */
 167:MassStorageHost.c **** void MassStorageHost_Task(void)
 168:MassStorageHost.c **** {
 169:MassStorageHost.c **** 	if (USB_HostState != HOST_STATE_Configured)
 170:MassStorageHost.c **** 	  return;
 171:MassStorageHost.c **** 
 172:MassStorageHost.c **** 	/* Indicate device busy via the status LEDs */
 173:MassStorageHost.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_BUSY);
 174:MassStorageHost.c **** 
 175:MassStorageHost.c **** 	uint8_t ErrorCode;
 176:MassStorageHost.c **** 
 177:MassStorageHost.c **** 	/* Send the request, display error and wait for device detach if request fails */
 178:MassStorageHost.c **** 	if ((ErrorCode = MassStore_GetMaxLUN(&MassStore_MaxLUNIndex)) != HOST_SENDCONTROL_Successful)
 179:MassStorageHost.c **** 	{
 180:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Get Max LUN"), ErrorCode);
 181:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 182:MassStorageHost.c **** 		return;
 183:MassStorageHost.c **** 	}
 184:MassStorageHost.c **** 
 185:MassStorageHost.c **** 	/* Print number of LUNs detected in the attached device */
 186:MassStorageHost.c **** 	printf_P(PSTR("Total LUNs: %d - Using first LUN in device.\r\n"), (MassStore_MaxLUNIndex + 1));
 187:MassStorageHost.c **** 
 188:MassStorageHost.c **** 	/* Reset the Mass Storage device interface, ready for use */
 189:MassStorageHost.c **** 	if ((ErrorCode = MassStore_MassStorageReset()) != HOST_SENDCONTROL_Successful)
 190:MassStorageHost.c **** 	{
 191:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Mass Storage Reset"), ErrorCode);
 192:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 193:MassStorageHost.c **** 		return;
 194:MassStorageHost.c **** 	}
 195:MassStorageHost.c **** 
 196:MassStorageHost.c **** 	/* Get sense data from the device - many devices will not accept any other commands until the sens
 197:MassStorageHost.c **** 	 * is read - both on start-up and after a failed command */
 198:MassStorageHost.c **** 	SCSI_Request_Sense_Response_t SenseData;
 199:MassStorageHost.c **** 	if ((ErrorCode = MassStore_RequestSense(0, &SenseData)) != 0)
 200:MassStorageHost.c **** 	{
 201:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Request Sense"), ErrorCode);
 202:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 203:MassStorageHost.c **** 		return;
 204:MassStorageHost.c **** 	}
 205:MassStorageHost.c **** 
 206:MassStorageHost.c **** 	/* Set the prevent removal flag for the device, allowing it to be accessed */
 207:MassStorageHost.c **** 	if ((ErrorCode = MassStore_PreventAllowMediumRemoval(0, true)) != 0)
 208:MassStorageHost.c **** 	{
 209:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Prevent/Allow Medium Removal"), ErrorCode);
 210:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 211:MassStorageHost.c **** 		return;
 212:MassStorageHost.c **** 	}
 213:MassStorageHost.c **** 
 214:MassStorageHost.c **** 	/* Get inquiry data from the device */
 215:MassStorageHost.c **** 	SCSI_Inquiry_Response_t InquiryData;
 216:MassStorageHost.c **** 	if ((ErrorCode = MassStore_Inquiry(0, &InquiryData)) != 0)
 217:MassStorageHost.c **** 	{
 218:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Inquiry"), ErrorCode);
 219:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 220:MassStorageHost.c **** 		return;
 221:MassStorageHost.c **** 	}
 222:MassStorageHost.c **** 
 223:MassStorageHost.c **** 	/* Print vendor and product names of attached device */
 224:MassStorageHost.c **** 	printf_P(PSTR("Vendor \"%.8s\", Product \"%.16s\"\r\n"), InquiryData.VendorID, InquiryData.Product
 225:MassStorageHost.c **** 
 226:MassStorageHost.c **** 	/* Wait until disk ready */
 227:MassStorageHost.c **** 	puts_P(PSTR("Waiting until ready.."));
 228:MassStorageHost.c **** 
 229:MassStorageHost.c **** 	for (;;)
 230:MassStorageHost.c **** 	{
 231:MassStorageHost.c **** 		Serial_SendByte('.');
 232:MassStorageHost.c **** 
 233:MassStorageHost.c **** 		/* Abort if device removed */
 234:MassStorageHost.c **** 		if (USB_HostState == HOST_STATE_Unattached)
 235:MassStorageHost.c **** 		  break;
 236:MassStorageHost.c **** 
 237:MassStorageHost.c **** 		/* Check to see if the attached device is ready for new commands */
 238:MassStorageHost.c **** 		ErrorCode = MassStore_TestUnitReady(0);
 239:MassStorageHost.c **** 
 240:MassStorageHost.c **** 		/* If attached device is ready, abort the loop */
 241:MassStorageHost.c **** 		if (!(ErrorCode))
 242:MassStorageHost.c **** 		  break;
 243:MassStorageHost.c **** 
 244:MassStorageHost.c **** 		/* If an error other than a logical command failure (indicating device busy) returned, abort */
 245:MassStorageHost.c **** 		if (ErrorCode != MASS_STORE_SCSI_COMMAND_FAILED)
 246:MassStorageHost.c **** 		{
 247:MassStorageHost.c **** 			ShowDiskReadError(PSTR("Test Unit Ready"), ErrorCode);
 248:MassStorageHost.c **** 			USB_Host_SetDeviceConfiguration(0);
 249:MassStorageHost.c **** 			return;
 250:MassStorageHost.c **** 		}
 251:MassStorageHost.c **** 	}
 252:MassStorageHost.c **** 
 253:MassStorageHost.c **** 	puts_P(PSTR("\r\nRetrieving Capacity... "));
 254:MassStorageHost.c **** 
 255:MassStorageHost.c **** 	/* Create new structure for the disk's capacity in blocks and block size */
 256:MassStorageHost.c **** 	SCSI_Capacity_t DiskCapacity;
 257:MassStorageHost.c **** 
 258:MassStorageHost.c **** 	/* Retrieve disk capacity */
 259:MassStorageHost.c **** 	if ((ErrorCode = MassStore_ReadCapacity(0, &DiskCapacity)) != 0)
 260:MassStorageHost.c **** 	{
 261:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Read Capacity"), ErrorCode);
 262:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 263:MassStorageHost.c **** 		return;
 264:MassStorageHost.c **** 	}
 265:MassStorageHost.c **** 
 266:MassStorageHost.c **** 	/* Display the disk capacity in blocks * block size bytes */
 267:MassStorageHost.c **** 	printf_P(PSTR("%lu blocks of %lu bytes.\r\n"), DiskCapacity.Blocks, DiskCapacity.BlockSize);
 268:MassStorageHost.c **** 
 269:MassStorageHost.c **** 	/* Create a new buffer capable of holding a single block from the device */
 270:MassStorageHost.c **** 	uint8_t BlockBuffer[DiskCapacity.BlockSize];
 271:MassStorageHost.c **** 
 272:MassStorageHost.c **** 	/* Read in the first 512 byte block from the device */
 273:MassStorageHost.c **** 	if ((ErrorCode = MassStore_ReadDeviceBlock(0, 0x00000000, 1, DiskCapacity.BlockSize, BlockBuffer))
 274:MassStorageHost.c **** 	{
 275:MassStorageHost.c **** 		ShowDiskReadError(PSTR("Read Device Block"), ErrorCode);
 276:MassStorageHost.c **** 		USB_Host_SetDeviceConfiguration(0);
 277:MassStorageHost.c **** 		return;
 278:MassStorageHost.c **** 	}
 279:MassStorageHost.c **** 
 280:MassStorageHost.c **** 	puts_P(PSTR("\r\nContents of first block:\r\n"));
 281:MassStorageHost.c **** 
 282:MassStorageHost.c **** 	/* Print out the first block in both HEX and ASCII, 16 bytes per line */
 283:MassStorageHost.c **** 	for (uint16_t Chunk = 0; Chunk < (DiskCapacity.BlockSize >> 4); Chunk++)
 284:MassStorageHost.c **** 	{
 285:MassStorageHost.c **** 		/* Pointer to the start of the current 16-byte chunk in the read block of data */
 286:MassStorageHost.c **** 		uint8_t* ChunkPtr = &BlockBuffer[Chunk << 4];
 287:MassStorageHost.c **** 
 288:MassStorageHost.c **** 		/* Print out the 16 bytes of the chunk in HEX format */
 289:MassStorageHost.c **** 		for (uint8_t ByteOffset = 0; ByteOffset < (1 << 4); ByteOffset++)
 290:MassStorageHost.c **** 		{
 291:MassStorageHost.c **** 			char CurrByte = *(ChunkPtr + ByteOffset);
 292:MassStorageHost.c **** 
 293:MassStorageHost.c **** 			printf_P(PSTR("%.2X "), CurrByte);
 294:MassStorageHost.c **** 		}
 295:MassStorageHost.c **** 
 296:MassStorageHost.c **** 		puts_P(PSTR("    "));
 297:MassStorageHost.c **** 
 298:MassStorageHost.c **** 		/* Print out the 16 bytes of the chunk in ASCII format */
 299:MassStorageHost.c **** 		for (uint8_t ByteOffset = 0; ByteOffset < (1 << 4); ByteOffset++)
 300:MassStorageHost.c **** 		{
 301:MassStorageHost.c **** 			char CurrByte = *(ChunkPtr + ByteOffset);
 302:MassStorageHost.c **** 
 303:MassStorageHost.c **** 			putchar(isprint(CurrByte) ? CurrByte : '.');
 304:MassStorageHost.c **** 		}
 305:MassStorageHost.c **** 
 306:MassStorageHost.c **** 		puts_P(PSTR("\r\n"));
 307:MassStorageHost.c **** 	}
 308:MassStorageHost.c **** 
 309:MassStorageHost.c **** 	puts_P(PSTR("\r\n\r\nPress board button to read entire ASCII contents of disk...\r\n\r\n"));
 310:MassStorageHost.c **** 
 311:MassStorageHost.c **** 	/* Wait for the board button to be pressed */
 312:MassStorageHost.c **** 	while (!(Buttons_GetStatus() & BUTTONS_BUTTON1))
 313:MassStorageHost.c **** 	{
 314:MassStorageHost.c **** 		/* Abort if device removed */
 315:MassStorageHost.c **** 		if (USB_HostState == HOST_STATE_Unattached)
 316:MassStorageHost.c **** 		  return;
 317:MassStorageHost.c **** 	}
 318:MassStorageHost.c **** 
 319:MassStorageHost.c **** 	/* Print out the entire disk contents in ASCII format */
 320:MassStorageHost.c **** 	for (uint32_t CurrBlockAddress = 0; CurrBlockAddress < DiskCapacity.Blocks; CurrBlockAddress++)
 321:MassStorageHost.c **** 	{
 322:MassStorageHost.c **** 		/* Read in the next block of data from the device */
 323:MassStorageHost.c **** 		if ((ErrorCode = MassStore_ReadDeviceBlock(0, CurrBlockAddress, 1, DiskCapacity.BlockSize, BlockB
 324:MassStorageHost.c **** 		{
 325:MassStorageHost.c **** 			ShowDiskReadError(PSTR("Read Device Block"), ErrorCode);
 326:MassStorageHost.c **** 			USB_Host_SetDeviceConfiguration(0);
 327:MassStorageHost.c **** 			return;
 328:MassStorageHost.c **** 		}
 329:MassStorageHost.c **** 
 330:MassStorageHost.c **** 		/* Send the ASCII data in the read in block to the serial port */
 331:MassStorageHost.c **** 		for (uint16_t Byte = 0; Byte < DiskCapacity.BlockSize; Byte++)
 332:MassStorageHost.c **** 		{
 333:MassStorageHost.c **** 			char CurrByte = BlockBuffer[Byte];
 334:MassStorageHost.c **** 
 335:MassStorageHost.c **** 			putchar(isprint(CurrByte) ? CurrByte : '.');
 336:MassStorageHost.c **** 		}
 337:MassStorageHost.c **** 	}
 338:MassStorageHost.c **** 
 339:MassStorageHost.c **** 	/* Indicate device no longer busy */
 340:MassStorageHost.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_READY);
 341:MassStorageHost.c **** 	USB_Host_SetDeviceConfiguration(0);
 342:MassStorageHost.c **** }
