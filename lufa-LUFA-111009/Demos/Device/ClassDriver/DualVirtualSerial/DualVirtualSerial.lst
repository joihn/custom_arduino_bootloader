  38:DualVirtualSerial.c **** 
  39:DualVirtualSerial.c **** /** LUFA CDC Class driver interface configuration and state information. This structure is
  40:DualVirtualSerial.c ****  *  passed to all CDC Class driver functions, so that multiple instances of the same class
  41:DualVirtualSerial.c ****  *  within a device can be differentiated from one another. This is for the first CDC interface,
  42:DualVirtualSerial.c ****  *  which sends strings to the host for each joystick movement.
  43:DualVirtualSerial.c ****  */
  44:DualVirtualSerial.c **** USB_ClassInfo_CDC_Device_t VirtualSerial1_CDC_Interface =
  45:DualVirtualSerial.c **** 	{
  46:DualVirtualSerial.c **** 		.Config =
  47:DualVirtualSerial.c **** 			{
  48:DualVirtualSerial.c **** 				.ControlInterfaceNumber           = 0,
  49:DualVirtualSerial.c **** 
  50:DualVirtualSerial.c **** 				.DataINEndpointNumber             = CDC1_TX_EPNUM,
  51:DualVirtualSerial.c **** 				.DataINEndpointSize               = CDC_TXRX_EPSIZE,
  52:DualVirtualSerial.c **** 				.DataINEndpointDoubleBank         = false,
  53:DualVirtualSerial.c **** 
  54:DualVirtualSerial.c **** 				.DataOUTEndpointNumber            = CDC1_RX_EPNUM,
  55:DualVirtualSerial.c **** 				.DataOUTEndpointSize              = CDC_TXRX_EPSIZE,
  56:DualVirtualSerial.c **** 				.DataOUTEndpointDoubleBank        = false,
  57:DualVirtualSerial.c **** 
  58:DualVirtualSerial.c **** 				.NotificationEndpointNumber       = CDC1_NOTIFICATION_EPNUM,
  59:DualVirtualSerial.c **** 				.NotificationEndpointSize         = CDC_NOTIFICATION_EPSIZE,
  60:DualVirtualSerial.c **** 				.NotificationEndpointDoubleBank   = false,
  61:DualVirtualSerial.c **** 			},
  62:DualVirtualSerial.c **** 	};
  63:DualVirtualSerial.c **** 
  64:DualVirtualSerial.c **** /** LUFA CDC Class driver interface configuration and state information. This structure is
  65:DualVirtualSerial.c ****  *  passed to all CDC Class driver functions, so that multiple instances of the same class
  66:DualVirtualSerial.c ****  *  within a device can be differentiated from one another. This is for the second CDC interface,
  67:DualVirtualSerial.c ****  *  which echos back all received data from the host.
  68:DualVirtualSerial.c ****  */
  69:DualVirtualSerial.c **** USB_ClassInfo_CDC_Device_t VirtualSerial2_CDC_Interface =
  70:DualVirtualSerial.c **** 	{
  71:DualVirtualSerial.c **** 		.Config =
  72:DualVirtualSerial.c **** 			{
  73:DualVirtualSerial.c **** 				.ControlInterfaceNumber           = 2,
  74:DualVirtualSerial.c **** 
  75:DualVirtualSerial.c **** 				.DataINEndpointNumber             = CDC2_TX_EPNUM,
  76:DualVirtualSerial.c **** 				.DataINEndpointSize               = CDC_TXRX_EPSIZE,
  77:DualVirtualSerial.c **** 				.DataINEndpointDoubleBank         = false,
  78:DualVirtualSerial.c **** 
  79:DualVirtualSerial.c **** 				.DataOUTEndpointNumber            = CDC2_RX_EPNUM,
  80:DualVirtualSerial.c **** 				.DataOUTEndpointSize              = CDC_TXRX_EPSIZE,
  81:DualVirtualSerial.c **** 				.DataOUTEndpointDoubleBank        = false,
  82:DualVirtualSerial.c **** 
  83:DualVirtualSerial.c **** 				.NotificationEndpointNumber       = CDC2_NOTIFICATION_EPNUM,
  84:DualVirtualSerial.c **** 				.NotificationEndpointSize         = CDC_NOTIFICATION_EPSIZE,
  85:DualVirtualSerial.c **** 				.NotificationEndpointDoubleBank   = false,
  86:DualVirtualSerial.c **** 			},
  87:DualVirtualSerial.c **** 	};
  88:DualVirtualSerial.c **** 
  89:DualVirtualSerial.c **** 
  90:DualVirtualSerial.c **** /** Main program entry point. This routine contains the overall program flow, including initial
  91:DualVirtualSerial.c ****  *  setup of all components and the main program loop.
  92:DualVirtualSerial.c ****  */
  93:DualVirtualSerial.c **** int main(void)
  94:DualVirtualSerial.c **** {
  95:DualVirtualSerial.c **** 	SetupHardware();
  96:DualVirtualSerial.c **** 
  97:DualVirtualSerial.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_NOTREADY);
  98:DualVirtualSerial.c **** 	sei();
  99:DualVirtualSerial.c **** 
 100:DualVirtualSerial.c **** 	for (;;)
 101:DualVirtualSerial.c **** 	{
 102:DualVirtualSerial.c **** 		CheckJoystickMovement();
 103:DualVirtualSerial.c **** 
 104:DualVirtualSerial.c **** 		/* Discard all received data on the first CDC interface */
 105:DualVirtualSerial.c **** 		CDC_Device_ReceiveByte(&VirtualSerial1_CDC_Interface);
 106:DualVirtualSerial.c **** 
 107:DualVirtualSerial.c **** 		/* Echo all received data on the second CDC interface */
 108:DualVirtualSerial.c **** 		int16_t ReceivedByte = CDC_Device_ReceiveByte(&VirtualSerial2_CDC_Interface);
 109:DualVirtualSerial.c **** 		if (!(ReceivedByte < 0))
 110:DualVirtualSerial.c **** 		  CDC_Device_SendByte(&VirtualSerial2_CDC_Interface, (uint8_t)ReceivedByte);
 111:DualVirtualSerial.c **** 
 112:DualVirtualSerial.c **** 		CDC_Device_USBTask(&VirtualSerial1_CDC_Interface);
 113:DualVirtualSerial.c **** 		CDC_Device_USBTask(&VirtualSerial2_CDC_Interface);
 114:DualVirtualSerial.c **** 		USB_USBTask();
 115:DualVirtualSerial.c **** 	}
 116:DualVirtualSerial.c **** }
