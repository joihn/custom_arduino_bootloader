 138:BootloaderCDC.c **** /** Event handler for the USB_ControlRequest event. This is used to catch and process control reque
 139:BootloaderCDC.c ****  *  the device from the USB host before passing along unhandled control requests to the library for
 140:BootloaderCDC.c ****  *  internally.
 141:BootloaderCDC.c ****  */
 142:BootloaderCDC.c **** void EVENT_USB_Device_ControlRequest(void)
 143:BootloaderCDC.c **** {
 144:BootloaderCDC.c **** 	/* Ignore any requests that aren't directed to the CDC interface */
 145:BootloaderCDC.c **** 	if ((USB_ControlRequest.bmRequestType & (CONTROL_REQTYPE_TYPE | CONTROL_REQTYPE_RECIPIENT)) !=
 146:BootloaderCDC.c **** 	    (REQTYPE_CLASS | REQREC_INTERFACE))
 147:BootloaderCDC.c **** 	{
 148:BootloaderCDC.c **** 		return;
 149:BootloaderCDC.c **** 	}
 150:BootloaderCDC.c **** 
 151:BootloaderCDC.c **** 	/* Activity - toggle indicator LEDs */
 152:BootloaderCDC.c **** 	LEDs_ToggleLEDs(LEDS_LED1 | LEDS_LED2);
 153:BootloaderCDC.c **** 
 154:BootloaderCDC.c **** 	/* Process CDC specific control requests */
 155:BootloaderCDC.c **** 	switch (USB_ControlRequest.bRequest)
 156:BootloaderCDC.c **** 	{
 157:BootloaderCDC.c **** 		case CDC_REQ_GetLineEncoding:
 158:BootloaderCDC.c **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_DEVICETOHOST | REQTYPE_CLASS | REQREC_INTERFACE)
 159:BootloaderCDC.c **** 			{
 160:BootloaderCDC.c **** 				Endpoint_ClearSETUP();
 161:BootloaderCDC.c **** 
 162:BootloaderCDC.c **** 				/* Write the line coding data to the control endpoint */
 163:BootloaderCDC.c **** 				Endpoint_Write_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 164:BootloaderCDC.c **** 				Endpoint_ClearOUT();
 165:BootloaderCDC.c **** 			}
 166:BootloaderCDC.c **** 
 167:BootloaderCDC.c **** 			break;
 168:BootloaderCDC.c **** 		case CDC_REQ_SetLineEncoding:
 169:BootloaderCDC.c **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_HOSTTODEVICE | REQTYPE_CLASS | REQREC_INTERFACE)
 170:BootloaderCDC.c **** 			{
 171:BootloaderCDC.c **** 				Endpoint_ClearSETUP();
 172:BootloaderCDC.c **** 
 173:BootloaderCDC.c **** 				/* Read the line coding data in from the host into the global struct */
 174:BootloaderCDC.c **** 				Endpoint_Read_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 175:BootloaderCDC.c **** 				Endpoint_ClearIN();
 176:BootloaderCDC.c **** 			}
 177:BootloaderCDC.c **** 
 178:BootloaderCDC.c **** 			break;
 179:BootloaderCDC.c **** 	}
 180:BootloaderCDC.c **** }
