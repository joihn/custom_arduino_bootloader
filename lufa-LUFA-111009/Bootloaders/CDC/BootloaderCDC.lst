  31:BootloaderCDC.c **** /** \file
  32:BootloaderCDC.c ****  *
  33:BootloaderCDC.c ****  *  Main source file for the CDC class bootloader. This file contains the complete bootloader logic
  34:BootloaderCDC.c ****  */
  35:BootloaderCDC.c **** 
  36:BootloaderCDC.c **** #define  INCLUDE_FROM_BOOTLOADERCDC_C
  37:BootloaderCDC.c **** #include "BootloaderCDC.h"
  38:BootloaderCDC.c **** 
  39:BootloaderCDC.c **** /** Contains the current baud rate and other settings of the first virtual serial port. This must b
  40:BootloaderCDC.c ****  *  operating systems will not open the port unless the settings can be set successfully.
  41:BootloaderCDC.c ****  */
  42:BootloaderCDC.c **** static CDC_LineEncoding_t LineEncoding = { .BaudRateBPS = 0,
  43:BootloaderCDC.c ****                                            .CharFormat  = CDC_LINEENCODING_OneStopBit,
  44:BootloaderCDC.c ****                                            .ParityType  = CDC_PARITY_None,
  45:BootloaderCDC.c ****                                            .DataBits    = 8                            };
  46:BootloaderCDC.c **** 
  47:BootloaderCDC.c **** /** Current address counter. This stores the current address of the FLASH or EEPROM as set by the h
  48:BootloaderCDC.c ****  *  and is used when reading or writing to the AVRs memory (either FLASH or EEPROM depending on the
  49:BootloaderCDC.c ****  *  command.)
  50:BootloaderCDC.c ****  */
  51:BootloaderCDC.c **** static uint32_t CurrAddress;
  52:BootloaderCDC.c **** 
  53:BootloaderCDC.c **** /** Flag to indicate if the bootloader should be running, or should exit and allow the application 
  54:BootloaderCDC.c ****  *  via a watchdog reset. When cleared the bootloader will exit, starting the watchdog and entering
  55:BootloaderCDC.c ****  *  loop until the AVR restarts and the application runs.
  56:BootloaderCDC.c ****  */
  57:BootloaderCDC.c **** static bool RunBootloader = true;
  58:BootloaderCDC.c **** 
  59:BootloaderCDC.c **** 
  60:BootloaderCDC.c **** /** Main program entry point. This routine configures the hardware required by the bootloader, then
  61:BootloaderCDC.c ****  *  runs the bootloader processing routine until instructed to soft-exit, or hard-reset via the wat
  62:BootloaderCDC.c ****  *  the loaded application code.
  63:BootloaderCDC.c ****  */
  64:BootloaderCDC.c **** int main(void)
  65:BootloaderCDC.c **** {
  66:BootloaderCDC.c **** 	/* Setup hardware required for the bootloader */
  67:BootloaderCDC.c **** 	SetupHardware();
  68:BootloaderCDC.c **** 
  69:BootloaderCDC.c **** 	/* Turn on first LED on the board to indicate that the bootloader has started */
  70:BootloaderCDC.c **** 	LEDs_SetAllLEDs(LEDS_LED1);
  71:BootloaderCDC.c **** 
  72:BootloaderCDC.c **** 	/* Enable global interrupts so that the USB stack can function */
  73:BootloaderCDC.c **** 	sei();
  74:BootloaderCDC.c **** 
  75:BootloaderCDC.c **** 	while (RunBootloader)
  76:BootloaderCDC.c **** 	{
  77:BootloaderCDC.c **** 		CDC_Task();
  78:BootloaderCDC.c **** 		USB_USBTask();
  79:BootloaderCDC.c **** 	}
  80:BootloaderCDC.c **** 
  81:BootloaderCDC.c **** 	/* Disconnect from the host - USB interface will be reset later along with the AVR */
  82:BootloaderCDC.c **** 	USB_Detach();
  83:BootloaderCDC.c **** 
  84:BootloaderCDC.c **** 	/* Enable the watchdog and force a timeout to reset the AVR */
  85:BootloaderCDC.c **** 	wdt_enable(WDTO_250MS);
  86:BootloaderCDC.c **** 
  87:BootloaderCDC.c **** 	for (;;);
  88:BootloaderCDC.c **** }
  89:BootloaderCDC.c **** 
  90:BootloaderCDC.c **** /** Configures all hardware required for the bootloader. */
  91:BootloaderCDC.c **** void SetupHardware(void)
  92:BootloaderCDC.c **** {
  93:BootloaderCDC.c **** 	/* Disable watchdog if enabled by bootloader/fuses */
  94:BootloaderCDC.c **** 	MCUSR &= ~(1 << WDRF);
  95:BootloaderCDC.c **** 	wdt_disable();
  96:BootloaderCDC.c **** 
  97:BootloaderCDC.c **** 	/* Disable clock division */
  98:BootloaderCDC.c **** 	clock_prescale_set(clock_div_1);
  99:BootloaderCDC.c **** 
 100:BootloaderCDC.c **** 	/* Relocate the interrupt vector table to the bootloader section */
 101:BootloaderCDC.c **** 	MCUCR = (1 << IVCE);
 102:BootloaderCDC.c **** 	MCUCR = (1 << IVSEL);
 103:BootloaderCDC.c **** 
 104:BootloaderCDC.c **** 	/* Initialize USB Subsystem */
 105:BootloaderCDC.c **** 	USB_Init();
 106:BootloaderCDC.c **** 	LEDs_Init();
 107:BootloaderCDC.c **** 	
 108:BootloaderCDC.c **** 	/* Bootloader active LED toggle timer initialization */
 109:BootloaderCDC.c **** 	TIMSK1 = (1 << TOIE1);
 110:BootloaderCDC.c **** 	TCCR1B = ((1 << CS11) | (1 << CS10));	
 111:BootloaderCDC.c **** }
 112:BootloaderCDC.c **** 
 113:BootloaderCDC.c **** /** ISR to periodically toggle the LEDs on the board to indicate that the bootloader is active. */
 114:BootloaderCDC.c **** ISR(TIMER1_OVF_vect, ISR_BLOCK)
 115:BootloaderCDC.c **** {
 116:BootloaderCDC.c **** 	LEDs_ToggleLEDs(LEDS_LED1 | LEDS_LED2);
 117:BootloaderCDC.c **** }
