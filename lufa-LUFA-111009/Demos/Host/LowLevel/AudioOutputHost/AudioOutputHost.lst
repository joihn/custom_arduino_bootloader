 278:../../../../LUFA/Drivers/Peripheral/AVR8/ADC_AVR8.h **** 			{
 279:../../../../LUFA/Drivers/Peripheral/AVR8/ADC_AVR8.h **** 				#if (defined(__AVR_AT90USB1286__) || defined(__AVR_AT90USB646__) || \
