  76:USBtoSerial.c **** /** Main program entry point. This routine contains the overall program flow, including initial
  77:USBtoSerial.c ****  *  setup of all components and the main program loop.
  78:USBtoSerial.c ****  */
  79:USBtoSerial.c **** int main(void)
  80:USBtoSerial.c **** {
  81:USBtoSerial.c **** 	SetupHardware();
  82:USBtoSerial.c **** 
  83:USBtoSerial.c **** 	RingBuffer_InitBuffer(&USBtoUSART_Buffer, USBtoUSART_Buffer_Data, sizeof(USBtoUSART_Buffer_Data));
  84:USBtoSerial.c **** 	RingBuffer_InitBuffer(&USARTtoUSB_Buffer, USARTtoUSB_Buffer_Data, sizeof(USARTtoUSB_Buffer_Data));
  85:USBtoSerial.c **** 
  86:USBtoSerial.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_NOTREADY);
  87:USBtoSerial.c **** 	sei();
  88:USBtoSerial.c **** 
  89:USBtoSerial.c **** 	for (;;)
  90:USBtoSerial.c **** 	{
  91:USBtoSerial.c **** 		/* Only try to read in bytes from the CDC interface if the transmit buffer is not full */
  92:USBtoSerial.c **** 		if (!(RingBuffer_IsFull(&USBtoUSART_Buffer)))
  93:USBtoSerial.c **** 		{
  94:USBtoSerial.c **** 			int16_t ReceivedByte = CDC_Device_ReceiveByte(&VirtualSerial_CDC_Interface);
  95:USBtoSerial.c **** 
  96:USBtoSerial.c **** 			/* Read bytes from the USB OUT endpoint into the USART transmit buffer */
  97:USBtoSerial.c **** 			if (!(ReceivedByte < 0))
  98:USBtoSerial.c **** 			  RingBuffer_Insert(&USBtoUSART_Buffer, ReceivedByte);
  99:USBtoSerial.c **** 		}
 100:USBtoSerial.c **** 		
 101:USBtoSerial.c **** 		/* Check if the UART receive buffer flush timer has expired or the buffer is nearly full */
 102:USBtoSerial.c **** 		uint16_t BufferCount = RingBuffer_GetCount(&USARTtoUSB_Buffer);
 103:USBtoSerial.c **** 		if ((TIFR0 & (1 << TOV0)) || (BufferCount > (uint8_t)(sizeof(USARTtoUSB_Buffer_Data) * .75)))
 104:USBtoSerial.c **** 		{
 105:USBtoSerial.c **** 			/* Clear flush timer expiry flag */
 106:USBtoSerial.c **** 			TIFR0 |= (1 << TOV0);
 107:USBtoSerial.c **** 
 108:USBtoSerial.c **** 			/* Read bytes from the USART receive buffer into the USB IN endpoint */
 109:USBtoSerial.c **** 			while (BufferCount--)
 110:USBtoSerial.c **** 			{
 111:USBtoSerial.c **** 				/* Try to send the next byte of data to the host, abort if there is an error without dequeuing 
 112:USBtoSerial.c **** 				if (CDC_Device_SendByte(&VirtualSerial_CDC_Interface,
 113:USBtoSerial.c **** 				                        RingBuffer_Peek(&USARTtoUSB_Buffer)) != ENDPOINT_READYWAIT_NoError)
 114:USBtoSerial.c **** 				{
 115:USBtoSerial.c **** 					break;
 116:USBtoSerial.c **** 				}
 117:USBtoSerial.c **** 
 118:USBtoSerial.c **** 				/* Dequeue the already sent byte from the buffer now we have confirmed that no transmission err
 119:USBtoSerial.c **** 				RingBuffer_Remove(&USARTtoUSB_Buffer);
 120:USBtoSerial.c **** 			}
 121:USBtoSerial.c **** 		}
 122:USBtoSerial.c **** 
 123:USBtoSerial.c **** 		/* Load the next byte from the USART transmit buffer into the USART */
 124:USBtoSerial.c **** 		if (!(RingBuffer_IsEmpty(&USBtoUSART_Buffer)))
 125:USBtoSerial.c **** 		  Serial_SendByte(RingBuffer_Remove(&USBtoUSART_Buffer));
 126:USBtoSerial.c **** 
 127:USBtoSerial.c **** 		CDC_Device_USBTask(&VirtualSerial_CDC_Interface);
 128:USBtoSerial.c **** 		USB_USBTask();
 129:USBtoSerial.c **** 	}
 130:USBtoSerial.c **** }
