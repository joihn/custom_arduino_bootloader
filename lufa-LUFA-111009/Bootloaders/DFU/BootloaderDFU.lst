  31:BootloaderDFU.c **** /** \file
  32:BootloaderDFU.c ****  *
  33:BootloaderDFU.c ****  *  Main source file for the DFU class bootloader. This file contains the complete bootloader logic
  34:BootloaderDFU.c ****  */
  35:BootloaderDFU.c **** 
  36:BootloaderDFU.c **** #define  INCLUDE_FROM_BOOTLOADER_C
  37:BootloaderDFU.c **** #include "BootloaderDFU.h"
  38:BootloaderDFU.c **** 
  39:BootloaderDFU.c **** /** Flag to indicate if the bootloader is currently running in secure mode, disallowing memory oper
  40:BootloaderDFU.c ****  *  other than erase. This is initially set to the value set by SECURE_MODE, and cleared by the boo
  41:BootloaderDFU.c ****  *  once a memory erase has completed in a bootloader session.
  42:BootloaderDFU.c ****  */
  43:BootloaderDFU.c **** static bool IsSecure = SECURE_MODE;
  44:BootloaderDFU.c **** 
  45:BootloaderDFU.c **** /** Flag to indicate if the bootloader should be running, or should exit and allow the application 
  46:BootloaderDFU.c ****  *  via a soft reset. When cleared, the bootloader will abort, the USB interface will shut down and
  47:BootloaderDFU.c ****  *  jumped to via an indirect jump to location 0x0000 (or other location specified by the host).
  48:BootloaderDFU.c ****  */
  49:BootloaderDFU.c **** static bool RunBootloader = true;
  50:BootloaderDFU.c **** 
  51:BootloaderDFU.c **** /** Flag to indicate if the bootloader is waiting to exit. When the host requests the bootloader to
  52:BootloaderDFU.c ****  *  jump to the application address it specifies, it sends two sequential commands which must be pr
  53:BootloaderDFU.c ****  *  acknowledged. Upon reception of the first the RunBootloader flag is cleared and the WaitForExit
  54:BootloaderDFU.c ****  *  causing the bootloader to wait for the final exit command before shutting down.
  55:BootloaderDFU.c ****  */
  56:BootloaderDFU.c **** static bool WaitForExit = false;
  57:BootloaderDFU.c **** 
  58:BootloaderDFU.c **** /** Current DFU state machine state, one of the values in the DFU_State_t enum. */
  59:BootloaderDFU.c **** static uint8_t DFU_State = dfuIDLE;
  60:BootloaderDFU.c **** 
  61:BootloaderDFU.c **** /** Status code of the last executed DFU command. This is set to one of the values in the DFU_Statu
  62:BootloaderDFU.c ****  *  each operation, and returned to the host when a Get Status DFU request is issued.
  63:BootloaderDFU.c ****  */
  64:BootloaderDFU.c **** static uint8_t DFU_Status = OK;
  65:BootloaderDFU.c **** 
  66:BootloaderDFU.c **** /** Data containing the DFU command sent from the host. */
  67:BootloaderDFU.c **** static DFU_Command_t SentCommand;
  68:BootloaderDFU.c **** 
  69:BootloaderDFU.c **** /** Response to the last issued Read Data DFU command. Unlike other DFU commands, the read command
  70:BootloaderDFU.c ****  *  requires a single byte response from the bootloader containing the read data when the next DFU_
  71:BootloaderDFU.c ****  *  is issued by the host.
  72:BootloaderDFU.c ****  */
  73:BootloaderDFU.c **** static uint8_t ResponseByte;
  74:BootloaderDFU.c **** 
  75:BootloaderDFU.c **** /** Pointer to the start of the user application. By default this is 0x0000 (the reset vector), how
  76:BootloaderDFU.c ****  *  may specify an alternate address when issuing the application soft-start command.
  77:BootloaderDFU.c ****  */
  78:BootloaderDFU.c **** static AppPtr_t AppStartPtr = (AppPtr_t)0x0000;
  79:BootloaderDFU.c **** 
  80:BootloaderDFU.c **** /** 64-bit flash page number. This is concatenated with the current 16-bit address on USB AVRs cont
  81:BootloaderDFU.c ****  *  64KB of flash memory.
  82:BootloaderDFU.c ****  */
  83:BootloaderDFU.c **** static uint8_t Flash64KBPage = 0;
  84:BootloaderDFU.c **** 
  85:BootloaderDFU.c **** /** Memory start address, indicating the current address in the memory being addressed (either FLAS
  86:BootloaderDFU.c ****  *  depending on the issued command from the host).
  87:BootloaderDFU.c ****  */
  88:BootloaderDFU.c **** static uint16_t StartAddr = 0x0000;
  89:BootloaderDFU.c **** 
  90:BootloaderDFU.c **** /** Memory end address, indicating the end address to read from/write to in the memory being addres
  91:BootloaderDFU.c ****  *  of EEPROM depending on the issued command from the host).
  92:BootloaderDFU.c ****  */
  93:BootloaderDFU.c **** static uint16_t EndAddr = 0x0000;
  94:BootloaderDFU.c **** 
  95:BootloaderDFU.c **** 
  96:BootloaderDFU.c **** /** Main program entry point. This routine configures the hardware required by the bootloader, then
  97:BootloaderDFU.c ****  *  runs the bootloader processing routine until instructed to soft-exit, or hard-reset via the wat
  98:BootloaderDFU.c ****  *  the loaded application code.
  99:BootloaderDFU.c ****  */
 100:BootloaderDFU.c **** int main(void)
 101:BootloaderDFU.c **** {
 102:BootloaderDFU.c **** 	/* Configure hardware required by the bootloader */
 103:BootloaderDFU.c **** 	SetupHardware();
 104:BootloaderDFU.c **** 
 105:BootloaderDFU.c **** 	#if ((BOARD == BOARD_XPLAIN) || (BOARD == BOARD_XPLAIN_REV1))
 106:BootloaderDFU.c **** 	/* Disable JTAG debugging */
 107:BootloaderDFU.c **** 	MCUCR |= (1 << JTD);
 108:BootloaderDFU.c **** 	MCUCR |= (1 << JTD);
 109:BootloaderDFU.c **** 
 110:BootloaderDFU.c **** 	/* Enable pull-up on the JTAG TCK pin so we can use it to select the mode */
 111:BootloaderDFU.c **** 	PORTF |= (1 << 4);
 112:BootloaderDFU.c **** 	Delay_MS(10);
 113:BootloaderDFU.c **** 
 114:BootloaderDFU.c **** 	/* If the TCK pin is not jumpered to ground, start the user application instead */
 115:BootloaderDFU.c **** 	RunBootloader = (!(PINF & (1 << 4)));
 116:BootloaderDFU.c **** 	
 117:BootloaderDFU.c **** 	/* Re-enable JTAG debugging */
 118:BootloaderDFU.c **** 	MCUCR &= ~(1 << JTD);
 119:BootloaderDFU.c **** 	MCUCR &= ~(1 << JTD);	
 120:BootloaderDFU.c **** 	#endif
 121:BootloaderDFU.c **** 
 122:BootloaderDFU.c **** 	/* Turn on first LED on the board to indicate that the bootloader has started */
 123:BootloaderDFU.c **** 	LEDs_SetAllLEDs(LEDS_LED1);
 124:BootloaderDFU.c **** 
 125:BootloaderDFU.c **** 	/* Enable global interrupts so that the USB stack can function */
 126:BootloaderDFU.c **** 	sei();
 127:BootloaderDFU.c **** 
 128:BootloaderDFU.c **** 	/* Run the USB management task while the bootloader is supposed to be running */
 129:BootloaderDFU.c **** 	while (RunBootloader || WaitForExit)
 130:BootloaderDFU.c **** 	  USB_USBTask();
 131:BootloaderDFU.c **** 
 132:BootloaderDFU.c **** 	/* Reset configured hardware back to their original states for the user application */
 133:BootloaderDFU.c **** 	ResetHardware();
 134:BootloaderDFU.c **** 
 135:BootloaderDFU.c **** 	/* Start the user application */
 136:BootloaderDFU.c **** 	AppStartPtr();
 137:BootloaderDFU.c **** }
 138:BootloaderDFU.c **** 
 139:BootloaderDFU.c **** /** Configures all hardware required for the bootloader. */
 140:BootloaderDFU.c **** void SetupHardware(void)
 141:BootloaderDFU.c **** {
 142:BootloaderDFU.c **** 	/* Disable watchdog if enabled by bootloader/fuses */
 143:BootloaderDFU.c **** 	MCUSR &= ~(1 << WDRF);
 144:BootloaderDFU.c **** 	wdt_disable();
 145:BootloaderDFU.c **** 
 146:BootloaderDFU.c **** 	/* Disable clock division */
 147:BootloaderDFU.c **** 	clock_prescale_set(clock_div_1);
 148:BootloaderDFU.c **** 
 149:BootloaderDFU.c **** 	/* Relocate the interrupt vector table to the bootloader section */
 150:BootloaderDFU.c **** 	MCUCR = (1 << IVCE);
 151:BootloaderDFU.c **** 	MCUCR = (1 << IVSEL);
 152:BootloaderDFU.c **** 
 153:BootloaderDFU.c **** 	/* Initialize the USB subsystem */
 154:BootloaderDFU.c **** 	USB_Init();
 155:BootloaderDFU.c **** 	LEDs_Init();
 156:BootloaderDFU.c **** 	
 157:BootloaderDFU.c **** 	/* Bootloader active LED toggle timer initialization */
 158:BootloaderDFU.c **** 	TIMSK1 = (1 << TOIE1);
 159:BootloaderDFU.c **** 	TCCR1B = ((1 << CS11) | (1 << CS10));
 160:BootloaderDFU.c **** }
 161:BootloaderDFU.c **** 
 162:BootloaderDFU.c **** /** Resets all configured hardware required for the bootloader back to their original states. */
 163:BootloaderDFU.c **** void ResetHardware(void)
 164:BootloaderDFU.c **** {
 165:BootloaderDFU.c **** 	/* Shut down the USB subsystem */
 166:BootloaderDFU.c **** 	USB_Disable();
 167:BootloaderDFU.c **** 
 168:BootloaderDFU.c **** 	/* Relocate the interrupt vector table back to the application section */
 169:BootloaderDFU.c **** 	MCUCR = (1 << IVCE);
 170:BootloaderDFU.c **** 	MCUCR = 0;
 171:BootloaderDFU.c **** }
 172:BootloaderDFU.c **** 
 173:BootloaderDFU.c **** /** ISR to periodically toggle the LEDs on the board to indicate that the bootloader is active. */
 174:BootloaderDFU.c **** ISR(TIMER1_OVF_vect, ISR_BLOCK)
 175:BootloaderDFU.c **** {
 176:BootloaderDFU.c **** 	LEDs_ToggleLEDs(LEDS_LED1 | LEDS_LED2);
 177:BootloaderDFU.c **** }
 178:BootloaderDFU.c **** 
 179:BootloaderDFU.c **** /** Event handler for the USB_ControlRequest event. This is used to catch and process control reque
 180:BootloaderDFU.c ****  *  the device from the USB host before passing along unhandled control requests to the library for
 181:BootloaderDFU.c ****  *  internally.
 182:BootloaderDFU.c ****  */
 183:BootloaderDFU.c **** void EVENT_USB_Device_ControlRequest(void)
 184:BootloaderDFU.c **** {	
 185:BootloaderDFU.c **** 	/* Ignore any requests that aren't directed to the DFU interface */
 186:BootloaderDFU.c **** 	if ((USB_ControlRequest.bmRequestType & (CONTROL_REQTYPE_TYPE | CONTROL_REQTYPE_RECIPIENT)) !=
 187:BootloaderDFU.c **** 	    (REQTYPE_CLASS | REQREC_INTERFACE))
 188:BootloaderDFU.c **** 	{
 189:BootloaderDFU.c **** 		return;
 190:BootloaderDFU.c **** 	}
 191:BootloaderDFU.c **** 
 192:BootloaderDFU.c **** 	/* Activity - toggle indicator LEDs */
 193:BootloaderDFU.c **** 	LEDs_ToggleLEDs(LEDS_LED1 | LEDS_LED2);
 194:BootloaderDFU.c **** 
 195:BootloaderDFU.c **** 	/* Get the size of the command and data from the wLength value */
 196:BootloaderDFU.c **** 	SentCommand.DataSize = USB_ControlRequest.wLength;
 197:BootloaderDFU.c **** 
 198:BootloaderDFU.c **** 	switch (USB_ControlRequest.bRequest)
 199:BootloaderDFU.c **** 	{
 200:BootloaderDFU.c **** 		case DFU_REQ_DNLOAD:
 201:BootloaderDFU.c **** 			Endpoint_ClearSETUP();
 202:BootloaderDFU.c **** 
 203:BootloaderDFU.c **** 			/* Check if bootloader is waiting to terminate */
 204:BootloaderDFU.c **** 			if (WaitForExit)
 205:BootloaderDFU.c **** 			{
 206:BootloaderDFU.c **** 				/* Bootloader is terminating - process last received command */
 207:BootloaderDFU.c **** 				ProcessBootloaderCommand();
 208:BootloaderDFU.c **** 
 209:BootloaderDFU.c **** 				/* Indicate that the last command has now been processed - free to exit bootloader */
 210:BootloaderDFU.c **** 				WaitForExit = false;
 211:BootloaderDFU.c **** 			}
 212:BootloaderDFU.c **** 
 213:BootloaderDFU.c **** 			/* If the request has a data stage, load it into the command struct */
 214:BootloaderDFU.c **** 			if (SentCommand.DataSize)
 215:BootloaderDFU.c **** 			{
 216:BootloaderDFU.c **** 				while (!(Endpoint_IsOUTReceived()))
 217:BootloaderDFU.c **** 				{
 218:BootloaderDFU.c **** 					if (USB_DeviceState == DEVICE_STATE_Unattached)
 219:BootloaderDFU.c **** 					  return;
 220:BootloaderDFU.c **** 				}
 221:BootloaderDFU.c **** 
 222:BootloaderDFU.c **** 				/* First byte of the data stage is the DNLOAD request's command */
 223:BootloaderDFU.c **** 				SentCommand.Command = Endpoint_Read_8();
 224:BootloaderDFU.c **** 
 225:BootloaderDFU.c **** 				/* One byte of the data stage is the command, so subtract it from the total data bytes */
 226:BootloaderDFU.c **** 				SentCommand.DataSize--;
 227:BootloaderDFU.c **** 
 228:BootloaderDFU.c **** 				/* Load in the rest of the data stage as command parameters */
 229:BootloaderDFU.c **** 				for (uint8_t DataByte = 0; (DataByte < sizeof(SentCommand.Data)) &&
 230:BootloaderDFU.c **** 				     Endpoint_BytesInEndpoint(); DataByte++)
 231:BootloaderDFU.c **** 				{
 232:BootloaderDFU.c **** 					SentCommand.Data[DataByte] = Endpoint_Read_8();
 233:BootloaderDFU.c **** 					SentCommand.DataSize--;
 234:BootloaderDFU.c **** 				}
 235:BootloaderDFU.c **** 
 236:BootloaderDFU.c **** 				/* Process the command */
 237:BootloaderDFU.c **** 				ProcessBootloaderCommand();
 238:BootloaderDFU.c **** 			}
 239:BootloaderDFU.c **** 
 240:BootloaderDFU.c **** 			/* Check if currently downloading firmware */
 241:BootloaderDFU.c **** 			if (DFU_State == dfuDNLOAD_IDLE)
 242:BootloaderDFU.c **** 			{
 243:BootloaderDFU.c **** 				if (!(SentCommand.DataSize))
 244:BootloaderDFU.c **** 				{
 245:BootloaderDFU.c **** 					DFU_State = dfuIDLE;
 246:BootloaderDFU.c **** 				}
 247:BootloaderDFU.c **** 				else
 248:BootloaderDFU.c **** 				{
 249:BootloaderDFU.c **** 					/* Throw away the filler bytes before the start of the firmware */
 250:BootloaderDFU.c **** 					DiscardFillerBytes(DFU_FILLER_BYTES_SIZE);
 251:BootloaderDFU.c **** 
 252:BootloaderDFU.c **** 					/* Throw away the packet alignment filler bytes before the start of the firmware */
 253:BootloaderDFU.c **** 					DiscardFillerBytes(StartAddr % FIXED_CONTROL_ENDPOINT_SIZE);
 254:BootloaderDFU.c **** 
 255:BootloaderDFU.c **** 					/* Calculate the number of bytes remaining to be written */
 256:BootloaderDFU.c **** 					uint16_t BytesRemaining = ((EndAddr - StartAddr) + 1);
 257:BootloaderDFU.c **** 
 258:BootloaderDFU.c **** 					if (IS_ONEBYTE_COMMAND(SentCommand.Data, 0x00))        // Write flash
 259:BootloaderDFU.c **** 					{
 260:BootloaderDFU.c **** 						/* Calculate the number of words to be written from the number of bytes to be written */
 261:BootloaderDFU.c **** 						uint16_t WordsRemaining = (BytesRemaining >> 1);
 262:BootloaderDFU.c **** 
 263:BootloaderDFU.c **** 						union
 264:BootloaderDFU.c **** 						{
 265:BootloaderDFU.c **** 							uint16_t Words[2];
 266:BootloaderDFU.c **** 							uint32_t Long;
 267:BootloaderDFU.c **** 						} CurrFlashAddress                 = {.Words = {StartAddr, Flash64KBPage}};
 268:BootloaderDFU.c **** 
 269:BootloaderDFU.c **** 						uint32_t CurrFlashPageStartAddress = CurrFlashAddress.Long;
 270:BootloaderDFU.c **** 						uint8_t  WordsInFlashPage          = 0;
 271:BootloaderDFU.c **** 
 272:BootloaderDFU.c **** 						while (WordsRemaining--)
 273:BootloaderDFU.c **** 						{
 274:BootloaderDFU.c **** 							/* Check if endpoint is empty - if so clear it and wait until ready for next packet */
 275:BootloaderDFU.c **** 							if (!(Endpoint_BytesInEndpoint()))
 276:BootloaderDFU.c **** 							{
 277:BootloaderDFU.c **** 								Endpoint_ClearOUT();
 278:BootloaderDFU.c **** 
 279:BootloaderDFU.c **** 								while (!(Endpoint_IsOUTReceived()))
 280:BootloaderDFU.c **** 								{
 281:BootloaderDFU.c **** 									if (USB_DeviceState == DEVICE_STATE_Unattached)
 282:BootloaderDFU.c **** 									  return;
 283:BootloaderDFU.c **** 								}
 284:BootloaderDFU.c **** 							}
 285:BootloaderDFU.c **** 
 286:BootloaderDFU.c **** 							/* Write the next word into the current flash page */
 287:BootloaderDFU.c **** 							boot_page_fill(CurrFlashAddress.Long, Endpoint_Read_16_LE());
 288:BootloaderDFU.c **** 
 289:BootloaderDFU.c **** 							/* Adjust counters */
 290:BootloaderDFU.c **** 							WordsInFlashPage      += 1;
 291:BootloaderDFU.c **** 							CurrFlashAddress.Long += 2;
 292:BootloaderDFU.c **** 
 293:BootloaderDFU.c **** 							/* See if an entire page has been written to the flash page buffer */
 294:BootloaderDFU.c **** 							if ((WordsInFlashPage == (SPM_PAGESIZE >> 1)) || !(WordsRemaining))
 295:BootloaderDFU.c **** 							{
 296:BootloaderDFU.c **** 								/* Commit the flash page to memory */
 297:BootloaderDFU.c **** 								boot_page_write(CurrFlashPageStartAddress);
 298:BootloaderDFU.c **** 								boot_spm_busy_wait();
 299:BootloaderDFU.c **** 
 300:BootloaderDFU.c **** 								/* Check if programming incomplete */
 301:BootloaderDFU.c **** 								if (WordsRemaining)
 302:BootloaderDFU.c **** 								{
 303:BootloaderDFU.c **** 									CurrFlashPageStartAddress = CurrFlashAddress.Long;
 304:BootloaderDFU.c **** 									WordsInFlashPage          = 0;
 305:BootloaderDFU.c **** 
 306:BootloaderDFU.c **** 									/* Erase next page's temp buffer */
 307:BootloaderDFU.c **** 									boot_page_erase(CurrFlashAddress.Long);
 308:BootloaderDFU.c **** 									boot_spm_busy_wait();
 309:BootloaderDFU.c **** 								}
 310:BootloaderDFU.c **** 							}
 311:BootloaderDFU.c **** 						}
 312:BootloaderDFU.c **** 
 313:BootloaderDFU.c **** 						/* Once programming complete, start address equals the end address */
 314:BootloaderDFU.c **** 						StartAddr = EndAddr;
 315:BootloaderDFU.c **** 
 316:BootloaderDFU.c **** 						/* Re-enable the RWW section of flash */
 317:BootloaderDFU.c **** 						boot_rww_enable();
 318:BootloaderDFU.c **** 					}
 319:BootloaderDFU.c **** 					else                                                   // Write EEPROM
 320:BootloaderDFU.c **** 					{
 321:BootloaderDFU.c **** 						while (BytesRemaining--)
 322:BootloaderDFU.c **** 						{
 323:BootloaderDFU.c **** 							/* Check if endpoint is empty - if so clear it and wait until ready for next packet */
 324:BootloaderDFU.c **** 							if (!(Endpoint_BytesInEndpoint()))
 325:BootloaderDFU.c **** 							{
 326:BootloaderDFU.c **** 								Endpoint_ClearOUT();
 327:BootloaderDFU.c **** 
 328:BootloaderDFU.c **** 								while (!(Endpoint_IsOUTReceived()))
 329:BootloaderDFU.c **** 								{
 330:BootloaderDFU.c **** 									if (USB_DeviceState == DEVICE_STATE_Unattached)
 331:BootloaderDFU.c **** 									  return;
 332:BootloaderDFU.c **** 								}
 333:BootloaderDFU.c **** 							}
 334:BootloaderDFU.c **** 
 335:BootloaderDFU.c **** 							/* Read the byte from the USB interface and write to to the EEPROM */
 336:BootloaderDFU.c **** 							eeprom_write_byte((uint8_t*)StartAddr, Endpoint_Read_8());
 337:BootloaderDFU.c **** 
 338:BootloaderDFU.c **** 							/* Adjust counters */
 339:BootloaderDFU.c **** 							StartAddr++;
 340:BootloaderDFU.c **** 						}
 341:BootloaderDFU.c **** 					}
 342:BootloaderDFU.c **** 
 343:BootloaderDFU.c **** 					/* Throw away the currently unused DFU file suffix */
 344:BootloaderDFU.c **** 					DiscardFillerBytes(DFU_FILE_SUFFIX_SIZE);
 345:BootloaderDFU.c **** 				}
 346:BootloaderDFU.c **** 			}
 347:BootloaderDFU.c **** 
 348:BootloaderDFU.c **** 			Endpoint_ClearOUT();
 349:BootloaderDFU.c **** 
 350:BootloaderDFU.c **** 			Endpoint_ClearStatusStage();
 351:BootloaderDFU.c **** 
 352:BootloaderDFU.c **** 			break;
 353:BootloaderDFU.c **** 		case DFU_REQ_UPLOAD:
 354:BootloaderDFU.c **** 			Endpoint_ClearSETUP();
 355:BootloaderDFU.c **** 
 356:BootloaderDFU.c **** 			while (!(Endpoint_IsINReady()))
 357:BootloaderDFU.c **** 			{
 358:BootloaderDFU.c **** 				if (USB_DeviceState == DEVICE_STATE_Unattached)
 359:BootloaderDFU.c **** 				  return;
 360:BootloaderDFU.c **** 			}
 361:BootloaderDFU.c **** 
 362:BootloaderDFU.c **** 			if (DFU_State != dfuUPLOAD_IDLE)
 363:BootloaderDFU.c **** 			{
 364:BootloaderDFU.c **** 				if ((DFU_State == dfuERROR) && IS_ONEBYTE_COMMAND(SentCommand.Data, 0x01))       // Blank Check
 365:BootloaderDFU.c **** 				{
 366:BootloaderDFU.c **** 					/* Blank checking is performed in the DFU_DNLOAD request - if we get here we've told the host
 367:BootloaderDFU.c **** 					   that the memory isn't blank, and the host is requesting the first non-blank address */
 368:BootloaderDFU.c **** 					Endpoint_Write_16_LE(StartAddr);
 369:BootloaderDFU.c **** 				}
 370:BootloaderDFU.c **** 				else
 371:BootloaderDFU.c **** 				{
 372:BootloaderDFU.c **** 					/* Idle state upload - send response to last issued command */
 373:BootloaderDFU.c **** 					Endpoint_Write_8(ResponseByte);
 374:BootloaderDFU.c **** 				}
 375:BootloaderDFU.c **** 			}
 376:BootloaderDFU.c **** 			else
 377:BootloaderDFU.c **** 			{
 378:BootloaderDFU.c **** 				/* Determine the number of bytes remaining in the current block */
 379:BootloaderDFU.c **** 				uint16_t BytesRemaining = ((EndAddr - StartAddr) + 1);
 380:BootloaderDFU.c **** 
 381:BootloaderDFU.c **** 				if (IS_ONEBYTE_COMMAND(SentCommand.Data, 0x00))            // Read FLASH
 382:BootloaderDFU.c **** 				{
 383:BootloaderDFU.c **** 					/* Calculate the number of words to be written from the number of bytes to be written */
 384:BootloaderDFU.c **** 					uint16_t WordsRemaining = (BytesRemaining >> 1);
 385:BootloaderDFU.c **** 
 386:BootloaderDFU.c **** 					union
 387:BootloaderDFU.c **** 					{
