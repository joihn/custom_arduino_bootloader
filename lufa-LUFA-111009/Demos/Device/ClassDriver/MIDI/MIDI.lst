  39:MIDI.c        **** /** LUFA MIDI Class driver interface configuration and state information. This structure is
  40:MIDI.c        ****  *  passed to all MIDI Class driver functions, so that multiple instances of the same class
  41:MIDI.c        ****  *  within a device can be differentiated from one another.
  42:MIDI.c        ****  */
  43:MIDI.c        **** USB_ClassInfo_MIDI_Device_t Keyboard_MIDI_Interface =
  44:MIDI.c        **** 	{
  45:MIDI.c        **** 		.Config =
  46:MIDI.c        **** 			{
  47:MIDI.c        **** 				.StreamingInterfaceNumber = 1,
  48:MIDI.c        **** 
  49:MIDI.c        **** 				.DataINEndpointNumber      = MIDI_STREAM_IN_EPNUM,
  50:MIDI.c        **** 				.DataINEndpointSize        = MIDI_STREAM_EPSIZE,
  51:MIDI.c        **** 				.DataINEndpointDoubleBank  = false,
  52:MIDI.c        **** 
  53:MIDI.c        **** 				.DataOUTEndpointNumber     = MIDI_STREAM_OUT_EPNUM,
  54:MIDI.c        **** 				.DataOUTEndpointSize       = MIDI_STREAM_EPSIZE,
  55:MIDI.c        **** 				.DataOUTEndpointDoubleBank = false,
  56:MIDI.c        **** 			},
  57:MIDI.c        **** 	};
  58:MIDI.c        **** 
  59:MIDI.c        **** 
  60:MIDI.c        **** /** Main program entry point. This routine contains the overall program flow, including initial
  61:MIDI.c        ****  *  setup of all components and the main program loop.
  62:MIDI.c        ****  */
  63:MIDI.c        **** int main(void)
  64:MIDI.c        **** {
  65:MIDI.c        **** 	SetupHardware();
  66:MIDI.c        **** 
  67:MIDI.c        **** 	LEDs_SetAllLEDs(LEDMASK_USB_NOTREADY);
  68:MIDI.c        **** 	sei();
  69:MIDI.c        **** 
  70:MIDI.c        **** 	for (;;)
  71:MIDI.c        **** 	{
  72:MIDI.c        **** 		CheckJoystickMovement();
  73:MIDI.c        **** 
  74:MIDI.c        **** 		MIDI_EventPacket_t ReceivedMIDIEvent;
  75:MIDI.c        **** 		while (MIDI_Device_ReceiveEventPacket(&Keyboard_MIDI_Interface, &ReceivedMIDIEvent))
  76:MIDI.c        **** 		{
  77:MIDI.c        **** 			if ((ReceivedMIDIEvent.Command == (MIDI_COMMAND_NOTE_ON >> 4)) && (ReceivedMIDIEvent.Data3 > 0))
  78:MIDI.c        **** 			  LEDs_SetAllLEDs(ReceivedMIDIEvent.Data2 > 64 ? LEDS_LED1 : LEDS_LED2);
  79:MIDI.c        **** 			else
  80:MIDI.c        **** 			  LEDs_SetAllLEDs(LEDS_NO_LEDS);
  81:MIDI.c        **** 		}
  82:MIDI.c        **** 
  83:MIDI.c        **** 		MIDI_Device_USBTask(&Keyboard_MIDI_Interface);
  84:MIDI.c        **** 		USB_USBTask();
  85:MIDI.c        **** 	}
  86:MIDI.c        **** }
