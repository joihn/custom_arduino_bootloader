  88:HIDReportViewer.c **** {
  89:HIDReportViewer.c **** 	if (USB_CurrentMode != USB_MODE_Host)
  90:HIDReportViewer.c **** 	  return;
  91:HIDReportViewer.c **** 	
  92:HIDReportViewer.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_BUSY);
  93:HIDReportViewer.c **** 	
  94:HIDReportViewer.c **** 	OutputReportSizes();
  95:HIDReportViewer.c **** 	OutputParsedReportItems();
  96:HIDReportViewer.c **** 	
  97:HIDReportViewer.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_READY);
  98:HIDReportViewer.c **** 	USB_Host_SetDeviceConfiguration(0);
  99:HIDReportViewer.c **** }
