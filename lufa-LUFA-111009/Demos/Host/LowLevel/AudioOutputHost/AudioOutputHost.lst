 292:../../../../LUFA/Drivers/Peripheral/AVR8/ADC_AVR8.h **** 					DDRD  &= ~(1 << 4);
 293:../../../../LUFA/Drivers/Peripheral/AVR8/ADC_AVR8.h **** 					DIDR2 |=  (1 << 0);
 294:../../../../LUFA/Drivers/Peripheral/AVR8/ADC_AVR8.h **** 				}
