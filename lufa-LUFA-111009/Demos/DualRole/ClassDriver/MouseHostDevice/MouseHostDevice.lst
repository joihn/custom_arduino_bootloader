  31:MouseHostDevice.c **** /** \file
  32:MouseHostDevice.c ****  *
  33:MouseHostDevice.c ****  *  Main source file for the MouseHostDevice demo. This file contains the main tasks of
  34:MouseHostDevice.c ****  *  the demo and is responsible for the overall control flow of the demo.
  35:MouseHostDevice.c ****  */
  36:MouseHostDevice.c **** 
  37:MouseHostDevice.c **** #include "MouseHostDevice.h"
  38:MouseHostDevice.c **** 
  39:MouseHostDevice.c **** /** Main program entry point. This routine configures the hardware required by the application, the
  40:MouseHostDevice.c ****  *  enters a loop to run the application tasks in sequence.
  41:MouseHostDevice.c ****  */
  42:MouseHostDevice.c **** int main(void)
  43:MouseHostDevice.c **** {
  44:MouseHostDevice.c **** 	SetupHardware();
  45:MouseHostDevice.c **** 
  46:MouseHostDevice.c **** 	puts_P(PSTR(ESC_FG_CYAN "Mouse Host/Device Demo running.\r\n" ESC_FG_WHITE));
  47:MouseHostDevice.c **** 
  48:MouseHostDevice.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_NOTREADY);
  49:MouseHostDevice.c **** 	sei();
  50:MouseHostDevice.c **** 
  51:MouseHostDevice.c **** 	for (;;)
  52:MouseHostDevice.c **** 	{
  53:MouseHostDevice.c **** 		/* Determine which USB mode we are currently in */
  54:MouseHostDevice.c **** 		if (USB_CurrentMode == USB_MODE_Host)
  55:MouseHostDevice.c **** 		{
  56:MouseHostDevice.c **** 			MouseHost_Task();
  57:MouseHostDevice.c **** 			HID_Host_USBTask(&Mouse_HID_Host_Interface);
  58:MouseHostDevice.c **** 		}
  59:MouseHostDevice.c **** 		else
  60:MouseHostDevice.c **** 		{
  61:MouseHostDevice.c **** 			HID_Device_USBTask(&Mouse_HID_Device_Interface);
  62:MouseHostDevice.c **** 		}
  63:MouseHostDevice.c **** 
  64:MouseHostDevice.c **** 		USB_USBTask();
  65:MouseHostDevice.c **** 	}
  66:MouseHostDevice.c **** }
