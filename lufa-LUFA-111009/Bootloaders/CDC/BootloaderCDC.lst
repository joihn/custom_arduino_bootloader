 182:BootloaderCDC.c **** #if !defined(NO_BLOCK_SUPPORT)
 183:BootloaderCDC.c **** /** Reads or writes a block of EEPROM or FLASH memory to or from the appropriate CDC data endpoint,
 184:BootloaderCDC.c ****  *  on the AVR910 protocol command issued.
 185:BootloaderCDC.c ****  *
 186:BootloaderCDC.c ****  *  \param[in] Command  Single character AVR910 protocol command indicating what memory operation t
 187:BootloaderCDC.c ****  */
 188:BootloaderCDC.c **** static void ReadWriteMemoryBlock(const uint8_t Command)
 189:BootloaderCDC.c **** {
 190:BootloaderCDC.c **** 	uint16_t BlockSize;
 191:BootloaderCDC.c **** 	char     MemoryType;
 192:BootloaderCDC.c **** 
 193:BootloaderCDC.c **** 	bool     HighByte = false;
 194:BootloaderCDC.c **** 	uint8_t  LowByte  = 0;
 195:BootloaderCDC.c **** 
 196:BootloaderCDC.c **** 	BlockSize  = (FetchNextCommandByte() << 8);
 197:BootloaderCDC.c **** 	BlockSize |=  FetchNextCommandByte();
 198:BootloaderCDC.c **** 
 199:BootloaderCDC.c **** 	MemoryType =  FetchNextCommandByte();
 200:BootloaderCDC.c **** 
 201:BootloaderCDC.c **** 	if ((MemoryType != 'E') && (MemoryType != 'F'))
 202:BootloaderCDC.c **** 	{
 203:BootloaderCDC.c **** 		/* Send error byte back to the host */
 204:BootloaderCDC.c **** 		WriteNextResponseByte('?');
 205:BootloaderCDC.c **** 
 206:BootloaderCDC.c **** 		return;
 207:BootloaderCDC.c **** 	}
 208:BootloaderCDC.c **** 
 209:BootloaderCDC.c **** 	/* Check if command is to read memory */
 210:BootloaderCDC.c **** 	if (Command == 'g')
 211:BootloaderCDC.c **** 	{
 212:BootloaderCDC.c **** 		/* Re-enable RWW section */
 213:BootloaderCDC.c **** 		boot_rww_enable();
 214:BootloaderCDC.c **** 
 215:BootloaderCDC.c **** 		while (BlockSize--)
 216:BootloaderCDC.c **** 		{
 217:BootloaderCDC.c **** 			if (MemoryType == 'F')
 218:BootloaderCDC.c **** 			{
 219:BootloaderCDC.c **** 				/* Read the next FLASH byte from the current FLASH page */
 220:BootloaderCDC.c **** 				#if (FLASHEND > 0xFFFF)
 221:BootloaderCDC.c **** 				WriteNextResponseByte(pgm_read_byte_far(CurrAddress | HighByte));
 222:BootloaderCDC.c **** 				#else
 223:BootloaderCDC.c **** 				WriteNextResponseByte(pgm_read_byte(CurrAddress | HighByte));
 224:BootloaderCDC.c **** 				#endif
 225:BootloaderCDC.c **** 
 226:BootloaderCDC.c **** 				/* If both bytes in current word have been read, increment the address counter */
 227:BootloaderCDC.c **** 				if (HighByte)
 228:BootloaderCDC.c **** 				  CurrAddress += 2;
 229:BootloaderCDC.c **** 
 230:BootloaderCDC.c **** 				HighByte = !HighByte;
 231:BootloaderCDC.c **** 			}
 232:BootloaderCDC.c **** 			else
 233:BootloaderCDC.c **** 			{
 234:BootloaderCDC.c **** 				/* Read the next EEPROM byte into the endpoint */
 235:BootloaderCDC.c **** 				WriteNextResponseByte(eeprom_read_byte((uint8_t*)(intptr_t)(CurrAddress >> 1)));
 236:BootloaderCDC.c **** 
 237:BootloaderCDC.c **** 				/* Increment the address counter after use */
 238:BootloaderCDC.c **** 				CurrAddress += 2;
 239:BootloaderCDC.c **** 			}
 240:BootloaderCDC.c **** 		}
 241:BootloaderCDC.c **** 	}
 242:BootloaderCDC.c **** 	else
 243:BootloaderCDC.c **** 	{
 244:BootloaderCDC.c **** 		uint32_t PageStartAddress = CurrAddress;
 245:BootloaderCDC.c **** 
 246:BootloaderCDC.c **** 		if (MemoryType == 'F')
 247:BootloaderCDC.c **** 		{
 248:BootloaderCDC.c **** 			boot_page_erase(PageStartAddress);
 249:BootloaderCDC.c **** 			boot_spm_busy_wait();
 250:BootloaderCDC.c **** 		}
 251:BootloaderCDC.c **** 
 252:BootloaderCDC.c **** 		while (BlockSize--)
 253:BootloaderCDC.c **** 		{
 254:BootloaderCDC.c **** 			if (MemoryType == 'F')
 255:BootloaderCDC.c **** 			{
 256:BootloaderCDC.c **** 				/* If both bytes in current word have been written, increment the address counter */
 257:BootloaderCDC.c **** 				if (HighByte)
 258:BootloaderCDC.c **** 				{
 259:BootloaderCDC.c **** 					/* Write the next FLASH word to the current FLASH page */
 260:BootloaderCDC.c **** 					boot_page_fill(CurrAddress, ((FetchNextCommandByte() << 8) | LowByte));
 261:BootloaderCDC.c **** 
 262:BootloaderCDC.c **** 					/* Increment the address counter after use */
 263:BootloaderCDC.c **** 					CurrAddress += 2;
 264:BootloaderCDC.c **** 				}
 265:BootloaderCDC.c **** 				else
 266:BootloaderCDC.c **** 				{
 267:BootloaderCDC.c **** 					LowByte = FetchNextCommandByte();
 268:BootloaderCDC.c **** 				}
 269:BootloaderCDC.c **** 				
 270:BootloaderCDC.c **** 				HighByte = !HighByte;
 271:BootloaderCDC.c **** 			}
 272:BootloaderCDC.c **** 			else
 273:BootloaderCDC.c **** 			{
 274:BootloaderCDC.c **** 				/* Write the next EEPROM byte from the endpoint */
 275:BootloaderCDC.c **** 				eeprom_write_byte((uint8_t*)((intptr_t)(CurrAddress >> 1)), FetchNextCommandByte());
 276:BootloaderCDC.c **** 
 277:BootloaderCDC.c **** 				/* Increment the address counter after use */
 278:BootloaderCDC.c **** 				CurrAddress += 2;
 279:BootloaderCDC.c **** 			}
 280:BootloaderCDC.c **** 		}
 281:BootloaderCDC.c **** 
 282:BootloaderCDC.c **** 		/* If in FLASH programming mode, commit the page after writing */
 283:BootloaderCDC.c **** 		if (MemoryType == 'F')
 284:BootloaderCDC.c **** 		{
 285:BootloaderCDC.c **** 			/* Commit the flash page to memory */
 286:BootloaderCDC.c **** 			boot_page_write(PageStartAddress);
 287:BootloaderCDC.c **** 
 288:BootloaderCDC.c **** 			/* Wait until write operation has completed */
 289:BootloaderCDC.c **** 			boot_spm_busy_wait();
 290:BootloaderCDC.c **** 		}
 291:BootloaderCDC.c **** 
 292:BootloaderCDC.c **** 		/* Send response byte back to the host */
 293:BootloaderCDC.c **** 		WriteNextResponseByte('\r');
 294:BootloaderCDC.c **** 	}
 295:BootloaderCDC.c **** }
 296:BootloaderCDC.c **** #endif
