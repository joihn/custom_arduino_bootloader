  80:MIDIToneGenerator.c **** /** Array of structures describing each note being generated */
  81:MIDIToneGenerator.c **** static DDSNoteData NoteData[MAX_SIMULTANEOUS_NOTES];
  82:MIDIToneGenerator.c **** 
  83:MIDIToneGenerator.c **** 
  84:MIDIToneGenerator.c **** /** Main program entry point. This routine contains the overall program flow, including initial
  85:MIDIToneGenerator.c ****  *  setup of all components and the main program loop.
  86:MIDIToneGenerator.c ****  */
  87:MIDIToneGenerator.c **** int main(void)
  88:MIDIToneGenerator.c **** {
  89:MIDIToneGenerator.c **** 	SetupHardware();
  90:MIDIToneGenerator.c **** 
  91:MIDIToneGenerator.c **** 	LEDs_SetAllLEDs(LEDMASK_USB_NOTREADY);
  92:MIDIToneGenerator.c **** 	sei();
  93:MIDIToneGenerator.c **** 	
  94:MIDIToneGenerator.c **** 	for (;;)
  95:MIDIToneGenerator.c **** 	{
  96:MIDIToneGenerator.c **** 		MIDI_EventPacket_t ReceivedMIDIEvent;
  97:MIDIToneGenerator.c **** 		if (MIDI_Device_ReceiveEventPacket(&Keyboard_MIDI_Interface, &ReceivedMIDIEvent))
  98:MIDIToneGenerator.c **** 		{
  99:MIDIToneGenerator.c **** 			if ((ReceivedMIDIEvent.Command == (MIDI_COMMAND_NOTE_ON >> 4)) && ((ReceivedMIDIEvent.Data1 & 0x
 100:MIDIToneGenerator.c **** 			{
 101:MIDIToneGenerator.c **** 				DDSNoteData* LRUNoteStruct = &NoteData[0];
 102:MIDIToneGenerator.c **** 			
 103:MIDIToneGenerator.c **** 				/* Find a free entry in the note table to use for the note being turned on */
 104:MIDIToneGenerator.c **** 				for (uint8_t i = 0; i < MAX_SIMULTANEOUS_NOTES; i++)
 105:MIDIToneGenerator.c **** 				{
 106:MIDIToneGenerator.c **** 					/* Check if the note is unused */
 107:MIDIToneGenerator.c **** 					if (!(NoteData[i].Pitch))
 108:MIDIToneGenerator.c **** 					{
 109:MIDIToneGenerator.c **** 						/* If a note is unused, it's age is essentially infinite - always prefer unused not entries *
 110:MIDIToneGenerator.c **** 						LRUNoteStruct = &NoteData[i];
 111:MIDIToneGenerator.c **** 						break;
 112:MIDIToneGenerator.c **** 					}
 113:MIDIToneGenerator.c **** 					else if (NoteData[i].LRUAge >= LRUNoteStruct->LRUAge)
 114:MIDIToneGenerator.c **** 					{
 115:MIDIToneGenerator.c **** 						/* If an older entry that the current entry has been found, prefer overwriting that one */			
 116:MIDIToneGenerator.c **** 						LRUNoteStruct = &NoteData[i];
 117:MIDIToneGenerator.c **** 					}
 118:MIDIToneGenerator.c **** 					
 119:MIDIToneGenerator.c **** 					NoteData[i].LRUAge++;
 120:MIDIToneGenerator.c **** 				}
 121:MIDIToneGenerator.c **** 				
 122:MIDIToneGenerator.c **** 				/* Update the oldest note entry with the new note data and reset its age */
 123:MIDIToneGenerator.c **** 				LRUNoteStruct->Pitch          = ReceivedMIDIEvent.Data2;
 124:MIDIToneGenerator.c **** 				LRUNoteStruct->TableIncrement = (uint32_t)(BASE_INCREMENT * SCALE_FACTOR) +
 125:MIDIToneGenerator.c **** 						                         ((uint32_t)(BASE_INCREMENT * NOTE_OCTIVE_RATIO * SCALE_FACTOR) *
 126:MIDIToneGenerator.c **** 						                          (ReceivedMIDIEvent.Data2 - BASE_PITCH_INDEX));
 127:MIDIToneGenerator.c **** 				LRUNoteStruct->TablePosition  = 0;
 128:MIDIToneGenerator.c **** 				LRUNoteStruct->LRUAge         = 0;
 129:MIDIToneGenerator.c **** 
 130:MIDIToneGenerator.c **** 				/* Turn on indicator LED to indicate note generation activity */
 131:MIDIToneGenerator.c **** 				LEDs_SetAllLEDs(LEDS_LED1);
 132:MIDIToneGenerator.c **** 			}
 133:MIDIToneGenerator.c **** 			else if ((ReceivedMIDIEvent.Command == (MIDI_COMMAND_NOTE_OFF >> 4)) && ((ReceivedMIDIEvent.Data
 134:MIDIToneGenerator.c **** 			{
 135:MIDIToneGenerator.c **** 				bool FoundActiveNote = false;
 136:MIDIToneGenerator.c **** 			
 137:MIDIToneGenerator.c **** 				/* Find the note in the note table to turn off */
 138:MIDIToneGenerator.c **** 				for (uint8_t i = 0; i < MAX_SIMULTANEOUS_NOTES; i++)
 139:MIDIToneGenerator.c **** 				{
 140:MIDIToneGenerator.c **** 					if (NoteData[i].Pitch == ReceivedMIDIEvent.Data2)
 141:MIDIToneGenerator.c **** 					  NoteData[i].Pitch = 0;
 142:MIDIToneGenerator.c **** 					else if (NoteData[i].Pitch)
 143:MIDIToneGenerator.c **** 					  FoundActiveNote   = true;
 144:MIDIToneGenerator.c **** 				}
 145:MIDIToneGenerator.c **** 				
 146:MIDIToneGenerator.c **** 				/* If all notes off, turn off the indicator LED */
 147:MIDIToneGenerator.c **** 				if (!(FoundActiveNote))
 148:MIDIToneGenerator.c **** 				  LEDs_SetAllLEDs(LEDS_NO_LEDS);
 149:MIDIToneGenerator.c **** 			}
 150:MIDIToneGenerator.c **** 		}
 151:MIDIToneGenerator.c **** 	
 152:MIDIToneGenerator.c **** 		MIDI_Device_USBTask(&Keyboard_MIDI_Interface);
